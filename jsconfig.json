{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": ["src/components/*"],
      "@app-sass/*": ["src/assets/scss/*"],
      "@app-images/*": ["src/assets/images"],
      "@views/*": ["src/views/*"],
      "@store/*": ["src/store/*"],
      "@services/*": ["src/services/*"],
      "@mixins/*": ["src/mixins/*"],
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  }
}
